{
    "name": "silverstripe/installer",
    "type": "silverstripe-recipe",
    "description": "The SilverStripe Framework Installer",
    "require": {
        "php": ">=7.1.0",
        "silverstripe/recipe-plugin": "^1.2",
        "silverstripe/recipe-cms": "4.x-dev",
        "silverstripe-themes/simple": "~3.2.0",
        "silverstripe/login-forms": "^4.0",
        "silverstripe/graphql": "4.x-dev as 3.x-dev",
        "silverstripe/versioned": "dev-pulls/1/schemageddon as 1.x-dev",
        "silverstripe/admin": "dev-pulls/1/schemageddon as 1.x-dev",
        "silverstripe/asset-admin": "dev-pulls/1/schemageddon as 1.x-dev",
        "silverstripe/cms": "dev-pulls/4/schemageddon as 4.x-dev",
        "silverstripe/framework": "dev-pulls/4/schemageddon as 4.x-dev",
        "silverstripe/graphql-devtools": "^1.0@dev",
        "dnadesign/silverstripe-elemental": "dev-pulls/4/schemageddon as 4.x-dev"
    },
    "repositories": {
        "silverstripe/admin": {
            "type": "vcs",
            "url": "git@github.com:/unclecheese/silverstripe-admin"
        },
        "silverstripe/asset-admin": {
            "type": "vcs",
            "url": "git@github.com:/unclecheese/silverstripe-asset-admin"
        },
        "silverstripe/versioned": {
            "type": "vcs",
            "url": "git@github.com:/unclecheese/silverstripe-versioned"
        },
        "silverstripe/frameworktest": {
            "type": "vcs",
            "url": "git@github.com:/unclecheese/silverstripe-frameworktest"
        },
        "silverstripe/cms": {
            "type": "vcs",
            "url": "git@github.com:/unclecheese/silverstripe-cms"
        },
        "silverstripe/framework": {
            "type": "vcs",
            "url": "git@github.com:/unclecheese/silverstripe-framework"
        },
        "silverstripe/elemental": {
            "type": "vcs",
            "url": "git@github.com:/unclecheese/silverstripe-elemental"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "sminnee/phpunit-mock-objects": "^3.4.5",
        "fzaninotto/faker": "^1.9"
    },
    "extra": {
        "resources-dir": "_resources",
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/database.yml",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "web.config"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
